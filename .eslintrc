{
	"root": true,
	"extends": [
		"eslint:recommended",
		"prettier",
		"plugin:prettier/recommended"
	],
	"plugins": [
		"react",
		"babel",
		"prettier"
	],
	"env": {
		"node": true,
		"es2021": true
	},
	"parser": "@babel/eslint-parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"settings": {
		"react": {
			"version": "detect"
		}
	}
}
